func ForLoop2.func1(): void {
  var a: int = 0
  {
    var i: int = 0
    while {
      var t~1: boolean = i<5
    } (t~1) {
      var t~2: int = a
      a = a+1
      var t~3: int = i
      i = i+1
    }
  }
  {
    var j: int = 0
    while {
      var t~4: boolean = j<a
    } (t~4) {
      a = 0
      var t~5: int = j
      j = j+1
    }
  }
}
